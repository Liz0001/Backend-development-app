<!DOCTYPE html>
<html>

<head>
	<title>Admin Page</title>
</head>

<body>


	<div style="width: 600px; margin: 0 auto; text-align:center">
		<header>
			<h1>MyApp</h1>
			<br>
			<hr>
		</header>

		<main>

			<h3>Admin Page</h3>
			<table style=" width:100%; text-align:center;">
				<thead>
					<tr>
						<th>User ID</th>
						<th>Name</th>
						<th>Role</th>
						<th>Password</th>
					</tr>
				</thead>
				<tbody>
					<% users.forEach(function(user) { %>
						<tr>
							<td>
								<%= user.userID %>
							</td>
							<td>
								<%= user.name %>
							</td>
							<td>
								<%= user.role %>
							</td>
							<td>
								<%= user.password %>
							</td>
						</tr>
						<% }); %>
				</tbody>
			</table>

			<br>
			<br>
			<hr>
			<div>
				<p><a href="/identify">Log Out</a></p>
			</div>
		</main>
	</div>


</body>

</html>